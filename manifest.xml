<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>a3f7160a-a2f4-482b-9784-47c37e387bd9</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>supplier.io</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="supplier.io"/>
  <Description DefaultValue="A template to get started."/>
  <IconUrl DefaultValue="https://username.github.io/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://username.github.io/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.supplier.io/contact-us"/>
  <AppDomains>
    <AppDomain>https://api.sustainably.ai/</AppDomain>
</AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://username.github.io/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="Suplier.io">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                        <bt:Image size="16" resid="Login.16x16" />
                        <bt:Image size="32" resid="Login.32x32" />
                        <bt:Image size="80" resid="Login.64x64" />
                  </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                        <bt:Image size="16" resid="Login.16x16" />
                        <bt:Image size="32" resid="Login.32x32" />
                        <bt:Image size="80" resid="Login.64x64" />
                    </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="GenerateButton">
                  <Label resid="GenerateButton.Label"/>
                  <Supertip>
                    <Title resid="GenerateButton.Label"/>
                    <Description resid="GenerateButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                        <bt:Image size="16" resid="Login.16x16" />
                        <bt:Image size="32" resid="Login.32x32" />
                        <bt:Image size="80" resid="Login.64x64" />
                    </Icon>
                  <Action xsi:type="ExecuteFunction">
                        <FunctionName>generate</FunctionName>
                    </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
          <ExtensionPoint xsi:type="ContextMenu">
            <OfficeMenu id="ContextMenuText">
                <Control xsi:type="Button" id="Button1Id1">
                    <Label resid="ContextMenu.Label" />
                    <Tooltip resid="ContextMenu.ToolTip" />
                    <Supertip>
                        <Title resid="ContextMenu.Label" />
                        <Description resid="ContextMenu.ToolTip" />
                    </Supertip>
                    <Icon>
                        <bt:Image size="16" resid="Login.16x16" />
                        <bt:Image size="32" resid="Login.32x32" />
                        <bt:Image size="80" resid="Login.64x64" />
                    </Icon>
                    <Action xsi:type="ExecuteFunction">
                        <FunctionName>generate</FunctionName>
                    </Action>
                </Control>
            </OfficeMenu>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Login.16x16" DefaultValue="https://username.github.io/assets/Login.16x16.png"/>
        <bt:Image id="Login.32x32" DefaultValue="https://username.github.io/assets/Login.32x32.png"/>
        <bt:Image id="Login.64x64" DefaultValue="https://username.github.io/assets/Login.64x64.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://username.github.io/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://username.github.io/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="CommandsGroup.Label" DefaultValue="supplier.io"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Log in"/>
        <bt:String id="GenerateButton.Label" DefaultValue="Generate"/>
        <bt:String id="ContextMenu.Label" DefaultValue="Generate"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Log in"/>
        <bt:String id="GenerateButton.Tooltip" DefaultValue="Generate selected text"/>
        <bt:String id="ContextMenu.ToolTip" DefaultValue="Generate selected text"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>